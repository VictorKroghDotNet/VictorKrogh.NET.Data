<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<Title>VictorKrogh.NET.Data</Title>
		<Description>Base implementation for all operations that handles data.</Description>
		<RepositoryUrl>https://github.com/VictorKroghDotNet/VictorKrogh.NET.Data</RepositoryUrl>
		<PackageReadmeFile>README.md</PackageReadmeFile>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="7.0.0" />
	  <PackageReference Include="VictorKrogh.NET.Disposable" Version="1.0.4" />
	</ItemGroup>

</Project>
